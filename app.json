    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.dndcampaignfinder.fresh"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
